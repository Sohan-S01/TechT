#include <iostream>
using namespace std;

void swap(float &x1, float &x2) {
    x1 = x1 + x2;
    x2 = x1 - x2;
    x1 = x1 - x2;
}

int main() {
    float x1, x2;
    cout << "Enter the two numbers: \n";
    cin >> x1 >> x2;
    swap(x1, x2);
    cout << "Swapped two numbers are: \n" << x1 << " " << x2 ;
    return 0;
}
