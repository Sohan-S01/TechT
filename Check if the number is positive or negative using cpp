#include <iostream>
#include <math.h>
using namespace std;

void check(int x){
    if(x>=0){
        cout<<"The number "<<x<< "positive";
    }
    else{
        cout<<"The number "<<x<< "negative";
    }
}

int main() {
    int x;
    cout<<"Enter the number :\n";
    cin >> x ;
    check(x);
    return 0;
}
