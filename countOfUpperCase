#include<stdio.h>
#include<ctype.h>
#include<string.h>
int main() 
{
    char regno[20];
    printf("Enter :");
    scanf("%[^\n]s",regno);
    int i,count=0;
    for(i=0;regno[i]!='\0';i++){
        if(isupper(regno[i]))
        count++;
    }
    printf("The count of upper case is : %d",count);
    return 0;
}
