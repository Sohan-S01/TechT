#include <iostream>
#include <string>
using namespace std;

int main() {
    string str, s;
    cout << "Enter the String to convert:\n";
    getline(cin, str);
    
    for (int i = 0; i < str.length(); i++) {
        if (!isspace(str[i])) {
            s += str[i];
        }
    }
    
    cout << "\nThe converted sentence is: " << s << endl;
    return 0;
}
