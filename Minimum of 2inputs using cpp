#include <iostream>
#include <math.h>
using namespace std;

void minimum(int x,int y){
    if(x<y){
        cout<<"The minimum number is "<<x;
    }
    else{
        cout<<"The minimum number is "<<y;
    }
}

int main() {
    int x,y;
    cout<<"Enter the numbers :\n";
    cin >> x >> y;
    minimum(x , y);
    return 0;
}
