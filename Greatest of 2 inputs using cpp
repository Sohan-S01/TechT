#include <iostream>
#include <math.h>
using namespace std;

void greatest(int x,int y){
    if(x>y){
        cout<<"The greatest number is "<<x;
    }
    else{
        cout<<"The greatest number is "<<y;
    }
}

int main() {
    int x,y;
    cout<<"Enter the numbers :\n";
    cin >> x >> y;
    greatest(x , y);
    return 0;
}
